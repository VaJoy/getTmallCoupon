{
  "manifest_version": 2,
  "name": "fuck tMall",
  "description": "抢天猫优惠券",
  "version": "1.0.1",
  "browser_action": {
    "default_icon": "img/redpack.png",
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": [
        "http://www.tmall.com/wow/*"
      ],
      "css": [
        "css/index.css"
      ],
      "js": [
        "js/zepto.js",
        "js/get.js"
      ]
    }
  ],
  "background": {
    "scripts": ["index.html"],
    "persistent": false
  },
  "permissions": [
    "http://www.tmall.com/"
  ]
}